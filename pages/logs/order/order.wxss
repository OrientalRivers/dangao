/* pages/logs/order/order.wxss */
.address_defalut_wrap.fixed .address_defalut {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    margin: 0 auto;
    max-width: 540px;
}
.address_defalut_wrap {
  background: #fff;
    position: relative;
    z-index: 320;
}
.address_defalut.address_border {
    padding-bottom: 16px;
    
}
.address_defalut {
    background: #fff;
    padding: 12px 10px;
    position: relative;
}
.address_defalut .ul {
    position: relative;
    padding-right: 30px;
}
.right_ico {
    content: "";
    display: inline-block;
    vertical-align: middle;
    margin-top: -2px;
    width: 10px;
    height: 14px;
    position: absolute;
    top: 50%;
    right: 0;
    margin-top: -5px;
}
.address_defalut .li {
    word-wrap: break-word;
    word-break: break-all;
    font-size: 14px;
}
.address_defalut .li .span.tag {
    display: inline-block;
    position: relative;
    overflow: hidden;
    padding: 0 5px;
    vertical-align: middle;
    margin: -2px 5px 0 0;
    max-width: 8em;
    height: 15px;
    line-height: 15px;
    font-size: 10px;
    color: #4b9bfb;
}
.address_defalut .li .span.tag::after {
    content: "";
    position: absolute;
    z-index: 1;
    pointer-events: none;
    background-color: #4b9bfb;
    border: 1px solid #ddd;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: none;
    border-color: #4b9bfb;
    border-radius: 2px;
}



.order_info {
    margin-top: 15px;
    margin-bottom: 15px;
    padding: 0 10px;
    background: #fff;
    position: relative;
}
.order_shopBar {
    display: block;
    position: relative;
    height: 40px;
    line-height: 40px;
    font-size: 16px;
    font-weight: 700;
}
.order_shopBar .order_shopBar_shop {
    display: inline-block;
    vertical-align: middle;
    margin-top: -2px;
    margin-right: 5px;
    width: 16px;
    height: 15px;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACAAAAAeBAMAAAC/JAEaAAAABGdBT…lxYxJDS/V2DwY1aTEFBnoBRgE0NrJ3wWwqCDCiRqUAhgAAC7dW0aXhPPwAAAAASUVORK5CYII=) no-repeat;
    background-size: 16px auto;
}
.order_info .hproduct {
    position: relative;
    min-height: 75px;
    padding: 0;
    font-size: 12px;
    padding-top: 15px;
}
.order_info .hproduct::before {
    content: "";
    position: absolute;
    z-index: 1;
    pointer-events: none;
    background-color: #e5e5e5;
    height: 1px;
    left: 0;
    right: 0;
    top: 0;
    left: -10px;
    right: -10px;
}
.order_info .hproduct .photo {
    width: 75px;
    height: 75px;
    position: absolute;
    top: 15px;
    left: 0;
}
.order_info .hproduct .fn, .order_info .hproduct .sku, .order_info .hproduct .sku_coll {
    padding-left: 85px;
}
.order_info .hproduct .fn {
    height: 40px;
    color: #333;
    line-height: 20px;
    margin-bottom: 5px;
    font-size: 14px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.order_info .hproduct .sku_coll {
    margin-bottom: 6px;
    font-size: 12px;
    color: #999;
    line-height: 1;
}
.order_info .hproduct .sku_price {
    padding-left: 85px;
    height: 30px;
    line-height: 30px;
    color: #e93b3d;
    font-size: 10px;
}
.order_info .hproduct .sku_price .span {
    font-size: 16px;
}
.order_info .hproduct .sku {
    height: 30px;
    margin-top: -30px;
    margin-bottom: 9px;
    text-align: right;
    word-wrap: break-word;
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.hproduct .sku .num_wrap {
    display: inline-block;
}
.num_wrap {
    display: inline-block;
    width: 107px;
    vertical-align: middle;
}
.order_info .hproduct .sku span {
    color: #333;
}
.num_wrap .minus {
    border-radius: 2px 0 0 2px;
}
.num_wrap input, .num_wrap .span {
    position: relative;
    float: left;
    width: 30px;
    height: 30px;
    line-height: 30px;
    background: #f7f7f7;
    text-align: center;
}
.num_wrap .minus:after, .num_wrap .plus:after, .num_wrap .plus:before {
    position: absolute;
    left: 50%;
    top: 50%;
    content: "";
    width: 12px;
    height: 2px;
    margin: -1px 0 0 -6px;
    background: #999;
}
.num_wrap .num {
    -webkit-appearance: none;
    border-radius: 0;
    width: 45px;
    height: 30px;
    line-height: normal;
    border: 0;
    border-left: 1px solid #fff;
    border-right: 1px solid #fff;
    background: #f7f7f7;
    font-size: 12px;
    text-align: center;
}
.num_wrap input, .num_wrap span {
    position: relative;
    float: left;
    width: 30px;
    height: 30px;
    line-height: 30px;
    background: #f7f7f7;
    text-align: center;
}
.num_wrap .plus:before {
    width: 2px;
    height: 12px;
    margin: -6px 0 0 -1px;
}



.order_info .order_info_list .li {
    padding: 10px 20px 10px 0;
    position: relative;
}
order_info .order_info_list .li:after {
    content: "";
    display: inline-block;
    vertical-align: middle;
    margin-top: -2px;
    width: 6px;
    height: 10px;
    background-image: url();
    background-repeat: no-repeat;
    background-size: 100%;
    position: absolute;
    right: 0;
    top: 50%;
    margin-top: -5px;
}
.order_info .shipping > .strong {
    position: absolute;
    left: 0;
    top: 12px;
    font-weight: 400;
    font-size: 16px;
}
.shipping_content {
    text-align: right;
}
.shipping_content .p {
  font-size: 12px;
  color: #333;
    margin: 5px 0;
}
.shipping_content .type_justify {
  font-size: 12px;
    overflow: hidden;
    margin: 0;
    color: #999;
}
.shipping_content .type_justify .left {
    float: left;
}
.order_info .order_info_guarantee {
    position: relative;
    padding: 12px 0;
}
.order_info .order_info_guarantee::before {
    content: "";
    position: absolute;
    z-index: 1;
    pointer-events: none;
    background-color: #e5e5e5;
    height: 1px;
    left: 0;
    right: 0;
    top: 0;
    right: -10px;
}
.order_info .order_info_guarantee .order_info_guarantee_title {
    margin-bottom: 3px;
    width: 62%;
    font-size: 16px;
    font-weight: 400;
    color: #333;
}
.order_info .order_info_guarantee .order_info_guarantee_content {
    width: 62%;
    font-size: 12px;
    color: #999;
}
.order_info .order_info_guarantee .order_info_guarantee_content .info {
    display: inline-block;
    margin: -2px 0 0 5px;
    width: 12px;
    height: 12px;
    vertical-align: middle;
    background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAB4AAAAeBAMAAADJHrORAAAABGdBT…2CMPei+wfdv+jhgRFe6OGJEd7A+FADxkcSLD4w4gs5PgEv+xzk6eKJOwAAAABJRU5ErkJggg==);
    background-size: 12px;
}
.order_info .order_info_guarantee .order_info_guarantee_price {
    position: absolute;
    right: 0;
    top: 50%;
    margin-top: -20px;
    padding-right: 20px;
    height: 40px;
    line-height: 40px;
    color: #e93b3d;
    font-size: 12px;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAB4AAAAeBAMAAADJHrORAAAABGdBT…9dPbp56Pahuwfdvej+QfcvenhghBd6eGKEN3p8YMQXcnwCAMmhLp/s1mG7AAAAAElFTkSuQmCC) no-repeat right 12px;
    background-size: 15px;
}


.buy_checkout {
    overflow: hidden;
    position: relative;
    background: #fff;
    margin-bottom: 15px;
}

.buy_checkout .li {
    position: relative;
    overflow: hidden;
}
.buy_checkout li:not(:last-child):before {
    content: "";
    position: absolute;
    z-index: 1;
    pointer-events: none;
    background-color: #e5e5e5;
    height: 1px;
    left: 0;
    right: 0;
    bottom: 0;
    left: 10px;
}
.buy_checkout .li .content {
    position: relative;
    display: block;
    text-align: right;
    font-size: 12px;
    color: #333;
    padding: 13px 30px 13px 75px;
}
.buy_checkout .li .strong {
    display: inline-block;
    font-weight: 400;
    position: absolute;
    left: 10px;
    top: 11px;
    font-size: 16px;
}
.buy_checkout .li .right_ico {
    content: "";
    display: inline-block;
    vertical-align: middle;
    margin-top: -2px;
    width: 10px;
    height: 14px;
    position: absolute;
    right: 10px;
    top: 50%;
    margin-top: -5px;
}
.buy_checkout .li em.disabled {
    color: #999;
}
.buy_checkout .li.disabled .buy_dou, .buy_checkout .li.disabled .content, .buy_checkout .li.disabled em {
    color: #999;
}
.buy_checkout .li:not(:last-child):before {
    content: "";
    position: absolute;
    z-index: 1;
    pointer-events: none;
    background-color: #e5e5e5;
    height: 1px;
    right: 0;
    bottom: 0;
    left: 10px;
}
.buy_checkout .mod_blockTips {
    margin: -1px 0;
    z-index: 1;
}
.mod_blockTips {
    position: relative;
    padding: 7px 10px;
    font-size: 14px;
    color: #de8c17!important;
    background: #fcf6ed;
}
.mod_blockTips .link, .mod_blockTips em {
    color: #999;
    font-size: 12px;
    line-height: 20px;
}



.buy_msg {
    position: relative;
    padding: 0 10px;
    width: 100%;
    height: 45px;
    margin: 15px 0;
    background-color: #fff;
    box-sizing: border-box;
}
.buy_msg_tit {
    position: absolute;
    top: 0;
    left: 10px;
    height: 45px;
    line-height: 45px;
    color: #333;
    font-size: 16px;
}
.buy_msg input {
    display: block;
    padding-left: 75px;
    width: 100%;
    font-size: 16px;
    height: 45px;
    line-height: 45px;
    box-sizing: border-box;
}
.buy_msg .tip {
    position: absolute;
    right: 5px;
    bottom: 2px;
    font-size: 10px;
    color: #ddd;
}

.buy_section {
    position: relative;
    overflow: hidden;
}
.buy_chart {
    position: relative;
    background-color: #fff;
    padding: 10px;
    font-size: 14px;
}
.buy_chart_item {
    display: flex;
}
.buy_chart_item_text {
    flex: 1;
    color: #333;
}
.buy_chart_item_price {
    color: #e93b3d;
}

.pay_area {
    overflow: hidden;
    text-align: center;
    padding: 0 10px;
    background: #fff;
    position: relative;
}
.pay_area .price {
    font-size: 16px;
    font-weight: 700;
    margin-bottom: 20px;
    text-align: right;
}
.pay_area .price .strong {
    color: #e93b3d;
    font-weight: 400;
}


.pay_area .mod_btns {
    margin: 10px 0;
}
.mod_btns .mod_btn.bg_wx {
    background: #06ba04;
    color: #fff;
    border-color: #06ba04;
}
.mod_btns .mod_btn.bg_pay {
    background: #bbebe9;
    color: #0f8783;
    border-color: #39c7c2;
}
.mod_btns .mod_btn {
    display: block;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    min-width: 0;
    height: 46px;
    line-height: 46px;
    text-align: center;
    font-size: 16px;
    border-radius: 4px;
    position: relative;
}


.buy_notsupport_title {
    font-size: 12px;
    color: #999;
    position: relative;
    font-weight: 400;
    margin: 20px 0 10px;
}
.buy_notsupport_list {
    text-align: left;
    position: relative;
    margin: 0 -10px;
}
.buy_notsupport_title:after, .buy_notsupport_title:before {
    content: "";
    height: 1px;
    width: 15px;
    background: #ddd;
    display: inline-block;
    vertical-align: middle;
    margin: -2px 5px 0;
}
.buy_notsupport_list {
    text-align: left;
    position: relative;
    margin: 0 -10px;
}
.buy_notsupport_list:before {
    content: "";
    position: absolute;
    z-index: 1;
    pointer-events: none;
    background-color: #e5e5e5;
    height: 1px;
    left: 0;
    right: 0;
    top: 0;
}
.buy_notsupport_list > .a, .buy_notsupport_list > .div {
    display: block;
    position: relative;
    padding: 10px 50px;
    overflow: hidden;
}
.buy_notsupport_list i.icon_delivery {
    background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAFAAAABQBAMAAAB8P++eAAAABGdBT…34i+ZhUiERXQ0TXbET3VQguvFBdHOG+AYS0U2uUTDoAABJ4v3/ot0uCgAAAABJRU5ErkJggg==);
}

.buy_notsupport_list i {
    display: block;
    width: 40px;
    height: 40px;
    position: absolute;
    top: 50%;
    margin-top: -20px;
    left: 6px;
    background-size: 100%;
}
.qq_footer, .wx_footer {
    padding: 25px 0 75px;
}
.qq_footer .jd_logo, .wx_footer .jd_logo {
    display: block;
    width: 100px;
    height: 20px;
    margin: 0 auto;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAMgAAAAoCAMAAACMwkUuAAAABGdBT…IN2+2rHNt6fOXMcv1et11yiT1UNZO/ud2uKYpSK60ssPwD8ruu+Ly+Rh0AAAAASUVORK5CYII=) no-repeat;
    background-size: 100%;
}